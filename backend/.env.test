# Test environment variables
NODE_ENV=test
LOG_LEVEL=debug

# Mock OAuth credentials for testing
GOOGLE_CLIENT_ID=test_client_id_123456789
GOOGLE_CLIENT_SECRET=test_client_secret_abcdefg
GOOGLE_REDIRECT_URI=http://localhost:3001/auth/callback

# JWT configuration for testing
JWT_SECRET=test_jwt_secret_placeholder
JWT_ISSUER=test-assistant-app
JWT_AUDIENCE=test-users
JWT_EXPIRES_IN=1h

# Database configuration (optional for unit tests)
DATABASE_URL=postgresql://test:test@localhost:5432/test_db

# Redis configuration (optional for unit tests)
REDIS_URL=redis://localhost:6379

# OpenAI configuration (mock for tests)
OPENAI_API_KEY=test_openai_key_placeholder

# Token encryption key for testing
TOKEN_ENCRYPTION_KEY=test_encryption_key_placeholder_32_chars_long

# Disable external services in tests
ENABLE_REAL_SERVICES=false
DISABLE_DATABASE=true
DISABLE_REDIS=true